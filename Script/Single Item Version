function getItemInfo(itemID) {
  var myUrl = "http://www.gw2spidy.com/api/v0.9/json/item/" + escape(itemID);
  var jsonData = UrlFetchApp.fetch(myUrl);
  var jsonString = jsonData.getContentText();
  var jsonObject = JSON.parse(jsonStr).result; // create object and remove "result" wrapper 

  var name = (jsonObject.name);
  var buyValue = (jsonObject.max_offer_unit_price);
  var sellValue = (jsonObject.min_sale_unit_price);
  var demand = (jsonObject.offer_availability);
  var supply = (jsonObject.sale_availability);
  var lastchange = (jsonObject.price_last_changed);
  var salechange = (jsonObject.sale_price_change_last_hour);
  var offerchange = (jsonObject.offer_price_change_last_hour);
  return [name, buyValue, sellValue, supply, demand, lastchange, salechange, offerchange]; //return data in spreadsheet cells
  
}
